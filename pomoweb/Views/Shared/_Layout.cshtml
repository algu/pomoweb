@using System.Security
<!DOCTYPE html>
<html>
    <head>
        <title>@ViewBag.Title</title>
        <link href="@Url.Content("~/Content/themes/Site.css")" rel="stylesheet" type="text/css" />
        <script src="@Url.Content("~/Scripts/timer.js")" type="text/javascript"></script>
        <script src="@Url.Content("~/Scripts/jquery-2.0.3.js")" type="text/javascript"></script>
        <script src="@Url.Content("~/Scripts/jquery.unobtrusive-ajax.js")" type="text/javascript"></script>
    </head>
    <body>
        <div class="timer">
            <div id="dial">
                25:00
            </div>
            <a class="button" id="start_btn">Start timer</a>
        </div>

        <br />
        <div class="bubble" id="main">
            @RenderBody()
        </div>
    </body>
</html>


